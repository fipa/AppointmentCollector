<h1>Listing dates</h1>

<table class="table table-hover table-bordered">
  <thead>
    <tr>
      <th>Fecha</th>
      <th>Hora inicio</th>
      <th>Hora termino</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @dates.each do |date| %>
      <tr>
        <td><%= date.date %></td>
        <td><%= date.begining %></td>
        <td><%= date.ending %></td>
        <td><%= link_to 'Ver detalles', date %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tenant', new_date_path %>
